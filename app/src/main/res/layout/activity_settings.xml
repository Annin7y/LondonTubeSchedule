<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/privacy_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/privacy_pol_text_start"
        android:layout_marginTop="@dimen/privacy_pol_text_top"
        android:textSize="@dimen/privacy_pol_set_text"
        android:textStyle="bold"
        android:focusable="false"
        android:text="@string/privacy_policy_settings"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
         />

    <TextView
        android:id="@+id/open_licenses_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/privacy_pol_text_start"
        android:layout_marginTop="@dimen/privacy_pol_text_top"
        android:textSize="@dimen/privacy_pol_set_text"
        android:textStyle="bold"
        android:focusable="false"
        android:text="@string/open_source_licenses_settings"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/privacy_text"
        />


</androidx.constraintlayout.widget.ConstraintLayout>
